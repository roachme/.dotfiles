# oh-my-zsh
bindkey -M viins 'jk' vi-cmd-mode # map jk to Esc

[ -s ~/.luaver/luaver ] && . ~/.luaver/luaver
[ -s ~/.luaver/completions/luaver.bash ] && . ~/.luaver/completions/luaver.bash

alias dev="sshpass -p isbetter ssh dev"
